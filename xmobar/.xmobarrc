Config { font = "xft:Iosevka Nerd Font Mono:style=Italic:size=9:antialias=true:hinting=true,xft:Deja Vu:size=9"
       , additionalFonts = ["xft:FontAwesome:size=10:antialias=true:hinting=true"]
       , borderColor = "black"
       , border = TopB
       , bgColor = "black"
       , fgColor = "grey"
       , alpha = 255
       , position = Top
       , textOffset = -1
       , iconOffset = -1
       , lowerOnStart = True
       , pickBroadest = False
       , persistent = False
       , hideOnStart = False
       , iconRoot = "."
       , allDesktops = True
       , overrideRedirect = True
       , commands = [ Run Network "eth1" ["-L","0","-H","32",
                                          "--normal","green","--high","red"] 10
                    -- , Run Volume "default" "Master" ["-t", "<fn=1></fn> <volume>% <status>", "--",
                    -- 			"-O", "", "-o", "mute"] 50
                    -- , Run Alsa "default" "Master" ["-t", " <volume>% <status>"]
                    -- 			"-O", "", "-o", "mute"] 50
                    , Run CoreTemp ["-t", " <core0>°C",
                    				"-L", "50", "-H", "80",
                    				"-l", "lightblue", "-n", "gray90", "-h", "red"] 50
   	   	   	   	    , Run BatteryP ["BAT0"]
                				["-t", "<acstatus><watts> (<left>%)",
                 	 	 	 	 "-L", "10", "-H", "80", "-p", "3",
                 	 	 	 	 "--", "-O", "<fc=green>On</fc> - ", "-i", "",
                 	 	 	 	 "-L", "-15", "-H", "-5",
                 	 	 	 	 "-l", "red", "-m", "blue", "-h", "green"]	600
                    , Run Cpu ["-L","3","-H","50",
                    				"--normal", "green", "--high", "red",
                    				"-t", "CPU <total>%"] 50
                    , Run MultiCpu ["-L","3","-H","50",
                    				"--normal", "green", "--high", "red",
                    				"-t", "CPU <total0>% | <total1>%"] 50
                    -- , Run Memory ["-t","RAM <usedratio>% <used>/<total> |"] 50
                    , Run Memory ["-t","RAM <usedratio>%"] 50
                    , Run Com "uname" ["-s","-r"] "" 36000
                    , Run Date "%_d-%m-%Y %H:%M:%S" "date" 10
                    , Run Brightness ["-t", "<bar>"] 50
                    , Run DynNetwork ["-t", "<dev>: <rx>KB|<tx>KB"] 50
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %StdinReader% } <fc=#ffffff>%date%</fc> { %coretemp% | %cpu% | %memory% | %dynnetwork% "
       }
