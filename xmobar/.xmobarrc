Config { font = "xft:Iosevka Nerd Font Mono:style=Italic:size=9:antialias=true:hinting=true,xft:Deja Vu:size=9"
       , additionalFonts = ["xft:FontAwesome:size=10:antialias=true:hinting=true"]
       , borderColor = "black"
       , border = TopB
       , bgColor = "black"
       , fgColor = "gray70"
       , alpha = 255
       , position = Top
       , textOffset = -1
       , iconOffset = -1
       , lowerOnStart = True
       , pickBroadest = False
       , persistent = False
       , hideOnStart = False
       , iconRoot = "."
       , allDesktops = True
       , overrideRedirect = True
       , commands = [ Run Network "eth1" ["-L","0","-H","32",
                                          "--normal","green","--high","red"] 10
                    -- , Run CoreTemp ["-t", " <core0>°C",
                    , Run CoreTemp ["-t", "TEMP <core0>°C",
                    				"-L", "50", "-H", "80",
                    				"-l", "lightblue", "-n", "gray70", "-h", "red"] 50
   	   	   	   	    , Run BatteryP ["BAT0"]
                				["-t", "<acstatus><watts> (<left>%)",
                 	 	 	 	 "-L", "10", "-H", "80", "-p", "3",
                 	 	 	 	 "--", "-O", "<fc=green>On</fc> - ", "-i", "",
                 	 	 	 	 "-L", "-15", "-H", "-5",
                 	 	 	 	 "-l", "red", "-m", "blue", "-h", "green"]	600
                    , Run Cpu ["-L","3","-H","50",
                               "--normal", "gray70", "--high", "red",
                               "-t", "CPU <total>%"] 50
                    -- , Run CpuFreq ["-t", "Freq:<cpu0>|<cpu1>GHz", "-L", "0", "-H", "1.2",
                    --    "-l", "lightblue", "-n","white", "-h", "red"] 50
                    -- , Run MultiCpu ["-L","3","-H","50",
                    -- 				"--normal", "green", "--high", "red",
                    -- 				"-t", "CPU <total0>% | <total1>%"] 50
                    -- , Run Memory ["-t","RAM <usedratio>% <used>/<total> |"] 50
                    , Run Memory ["-t","RAM <usedratio>%",
                    			  "-L", "10", "-H", "60",
                    			  "-l", "lightblue", "-h", "red"] 50
                    , Run Brightness ["-t", "<bar>"] 50
                    , Run DynNetwork ["-t", "<dev> <rx>KB|<tx>KB"] 50
                    , Run Date "%H:%M:%S" "date" 10
                    , Run Date "%_d-%m-%Y" "date2" 60
                    , Run Com "uname" ["-s","-r"] "" 36000
                    , Run Com "./scripts/get_volume.sh" [] "volume" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       -- , template = " %StdinReader% | %volume% } %date% { %cpufreq% | %coretemp% | %cpu% | %memory% | %dynnetwork% | %date2% "
       , template = " %StdinReader% | %dynnetwork% } %date% { %cpufreq% | %coretemp% | %cpu% | %memory% | %volume% | %date2% "
       }
